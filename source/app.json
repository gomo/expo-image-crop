{
  "expo": {
    "name": "Kropper",
    "slug": "expo-image-crop",
    "newArchEnabled": true,
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_crop.png",
    "splash": {
      "image": "./assets/crop_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#2683d9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
